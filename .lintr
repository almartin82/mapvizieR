linters: linters_with_defaults(
    line_length_linter(120),
    object_name_linter(styles = c("snake_case", "camelCase", "dotted.case")),
    commented_code_linter = NULL
  )
exclusions: list(
    "data-raw",
    "inst"
  )
exclude: "# nolint"
exclude_start: "# nolint start"
exclude_end: "# nolint end"
